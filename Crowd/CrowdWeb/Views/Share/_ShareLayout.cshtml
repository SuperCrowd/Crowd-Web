
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Crowd</title>
    <link href="~/Content/Share.css" rel="stylesheet" type="text/css">
</head>
<body>
    <header>
        <div class="container">
            <img id="logo" src="~/Content/logo.png"/>
           
        </div>
    </header>
    <div id="content" class="container body-content">
        @RenderBody()
        <hr />
        <footer>
            
        </footer>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
